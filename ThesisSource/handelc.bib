@InProceedings{icga85:cramer,
  Author         = {Cramer, {Nichael Lynn}},
  Title          = {A representation for the adaptive generation of simple
                   sequential programs},
  BookTitle      = {Proceedings of an {I}nternational {C}onference on
                   {G}enetic {A}lgorithms and their {A}pplications},
  Editor         = {Grefenstette, {John J.}},
  Pages          = {183--187},
  Address        = {Carnegie-Mellon University, Pittsburgh, PA, USA},
  url            = {ftp://ftp.bbn.com/pub/ncramer/gp/icga85.txt},
  abstract       = {An adaptive system for generating short sequential
                   computer functions is described. The created functions
                   are written in the simple "number-string" language JB,
                   and in TB, a modified version of JB with a tree-like
                   structure. These languages have the feature that they
                   can be used to represent well-formed, useful computer
                   programs while still being amenable to suitably defined
                   genetic operators. The system is used to produce
                   two-input, single-output multiplication functions that
                   are concise and well-defined. Future work, dealing with
                   extensions to more complicated functions and
                   generalizations of the techniques, is also discussed.},
  month          = {24-26 July},
  year           = 1985,
  keywords       = {genetic algorithms, genetic programming, memory},
  notes          = {The earliest description of the tree-like
