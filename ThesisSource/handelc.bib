@InProceedings{clack:1997:adcb,
  Author         = {Clack, Chris and Farringdon, Jonny and Lidwell, Peter
                   and Yu, Tina},
  Title          = {Autonomous document classification for business},
  BookTitle      = {The {F}irst {I}nternational {C}onference on
                   {A}utonomous {A}gents (agents '97)},
  Editor         = {Johnson, W. Lewis},
  Pages          = {201--208},
  Address        = {Marina del Rey, CA, USA},
  Publisher      = {ACM Press},
  isbn           = {0-89791-877-0},
  url            = {http://www.cs.ucl.ac.uk/staff/J.Farringdon/GP/Papers-agent97/agent97.html},
  organisation   = {ACM SIGART},
  year           = 1997,
  month          = {February 5-8},
  publisher_address = {1515 Broadway, New York, NY 10036, USA},
  keywords       = {genetic algorithms, genetic programming},
  notes          = {http://www.isi.edu/isd/AA97/info.html see also
                   clack:1996:adcb},
  abstract       = {With the continuing exponential growth of the Internet
                   and the more recent growth of business Intranets, the
                   commercial world is becoming increasingly aware of the
                   problem of electronic information overload. This has
                   encouraged interest in developing agents/softbots that
                   can act as electronic personal assistants and can
                   develop and adapt representations of users information
                   needs, commonly known as profiles. As the result of
                   collaborative research with Friends of the Earth, an
                   environmental issues campaigning organisation, we have
                   developed a general purpose information classification
                   agent architecture and have applied it to the problem
                   of document classification and routing. Collaboration
                   with Friends of the Earth allows us to test our ideas
                   in a non-academic context involving high volumes of
                   documents. We use the technique of genetic programming
                   (GP), (Koza and Rice 1992), to evolve classifying
                   agents. This is a novel approach for document
                   classification, where each agent evolves a parse-tree
                   representation of a user's particular information need.
                   The other unusual features of our research are the
                   longevity of our agents and the fact that they undergo
                   a continual training process; feedback from the user
                   enables the agent to adapt to the user's long-term
                   information requirements.},
  size           = {11 pages},
}

@InProceedings{yu:1997:pegp,
  Author         = {Clack, Chris and Yu, Tina},
  Title          = {Performance enhanced genetic programming},
  BookTitle      = {Proceedings of the {S}ixth {C}onference on
                   {E}volutionary {P}rogramming},
  Editor         = {Angeline, {Peter J.} and Reynolds, {Robert G.} and
                   McDonnell, {John R.} and Eberhart, Russ},
  Volume         = {1213},
  Series         = {LNCS},
  Address        = {Indianapolis, Indiana, USA},
  Publisher      = {Springer-Verlag},
  publisher_address = {Berlin},
  url            = {http://www.cs.ucl.ac.uk/staff/t.yu/ep97.ps},
  notes          = {EP-97},
  abstract       = {Genetic Programming is increasing in popularity as the
                   basis for a wide range of learning algorithms. However,
                   the technique has to date only been successfully
                   applied to modest tasks because of the performance
                   overheads of evolving a large number of data
                   structures, many of which do not correspond to a valid
                   program. We address this problem directly and
                   demonstrate how the evolutionary process can be
                   achieved with much greater efficiency through the use
                   of a formally-based representation and strong typing.
                   We report initial experimental results which
                   demonstrate that our technique exhibits significantly
                   better performance than previous work.},
  year           = 1997,
  keywords       = {genetic algorithms, genetic programming},
}

