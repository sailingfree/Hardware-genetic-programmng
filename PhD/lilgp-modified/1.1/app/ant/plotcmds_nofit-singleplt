set xlabel 'Program Length'
set ylabel 'Number of Programs'
set ytics rotate
maxyval=0
set key 
plot 'tmp/nofit-single_0.dat' title 'Gen 0' with linespoints
replot 'tmp/nofit-single_2.dat' title 'Gen 1' with linespoints
replot 'tmp/nofit-single_10.dat' title 'Gen 10' with linespoints
replot 'tmp/nofit-single_31.dat' title 'Gen 31' with linespoints
set output 'nofit-single.eps'
set term postscript eps 'Times-Roman' 24
replot
pause 10
